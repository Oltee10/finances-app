{
  "expo": {
    "name": "finances-app",
    "slug": "finances-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.jpg",
    "scheme": "financesapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./public/icon.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "icon": "./public/icon.jpg",
      "package": "com.olte.financesapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "display": "standalone",
      "scope": "/",
      "startUrl": "/finances",
      "favicon": "./public/apple-touch-icon.png",
      "backgroundColor": "#121212",
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent",
        "apple-mobile-web-app-title": "Mis Finanzas",
        "theme-color": "#121212"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./public/icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "baseUrl": "/finances"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7908369b-d771-4342-a2fd-0458ca6fa17f"
      }
    }
  }
}
